 public ruleBase RegrasPokemon {
 
 		rule AndarAleatoriamente {
		    declarations
		        AgenteMapa am;
		    conditions
		    	!am.getMapaElemento().existeElementoProximo(am.getAgente().getPosicao());
		    actions
		      am.getAgente().setDecisao(am.getAgente().ANDAR_ALEATORIAMENTE);
		      am.atualizaMapa();
		      modified(am);
		  }
  }
